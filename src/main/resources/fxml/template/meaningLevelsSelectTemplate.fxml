<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="containerHBox" alignment="CENTER_LEFT" spacing="1.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label text="Known meanings:">
      <HBox.margin>
         <Insets right="5.0" />
      </HBox.margin>
   </Label>
    <CheckBox fx:id="l1CheckBox" mnemonicParsing="false" text="1" />
    <CheckBox fx:id="l2CheckBox" mnemonicParsing="false" text="2" />
    <CheckBox fx:id="l3CheckBox" mnemonicParsing="false" text="3" />
    <CheckBox fx:id="l4CheckBox" mnemonicParsing="false" text="4" />
    <CheckBox fx:id="l5CheckBox" mnemonicParsing="false" text="5+" />
    <Button fx:id="selectAllButton" mnemonicParsing="false" onAction="#selectAllButtonClick" text="Select all" />
</fx:root>
